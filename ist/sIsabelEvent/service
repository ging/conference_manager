#--------------------------------------------------------------------
# INTERACTION MODE: quesmode
#--------------------------------------------------------------------

quesmode.whatis:      imode
quesmode.Audio:       -active 1 \
                      -codec "gsm-32KHz" \
                      -maxbw 300
quesmode.Questions:   -active 1 \
                      -resetlayout lresetquestions -resetothermap 0 \
                      -layout lquestions -othermap 0 \
                      -setaudio 0
quesmode.Video:       -active 1 -skip 1

#--------------------------------------------------------------------

Video.layout.lresetquestions: \
 {video 320x240+269+230 300}

Video.layout.lquestions: \
 {video 320x240+653+230 300} \
 {video 320x240+269+230 300}


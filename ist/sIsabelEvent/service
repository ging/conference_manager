#--------------------------------------------------------------------
# INTERACTION MODE: onemode
#--------------------------------------------------------------------

onemode.whatis:    imode
onemode.Audio:     -active 1 \
                   -codec "gsm-32KHz" \
                   -maxbw 300
onemode.Video:     -active 1 -layout lone-640 \
                   -otherlayout {ol3lq ol4lq ol5lq ol10lq ol25lq ol36lq}

# --------------------------------------------------------------------


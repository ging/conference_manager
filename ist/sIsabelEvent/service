# --------------------------------------------------------------------
#                    USER ACTIONS DEFINITION
# --------------------------------------------------------------------

IdleMode.whatis:    action
IdleMode.pos:       -row 0 -column 1
IdleMode.title:     @ng/psmall2.gif
IdleMode.balloon:   Idle mode
IdleMode.cmd:       SetIMode idlemode \
         "PRST Video ShDisplay Whiteboard Audio"


ChatMode.whatis:    action
ChatMode.pos:       -row 0 -column 2
ChatMode.title:     @ng/pall.gif
ChatMode.balloon:   Chat mode
ChatMode.cmd:       SetIMode chatmode \
         "PRST ShDisplay Fax Whiteboard Audio Video" *


OneMode.whatis:    action
OneMode.pos:        -row 0 -column 3
OneMode.title:     @ng/gc1.gif
OneMode.balloon:   One mode
OneMode.cmd:       SetIMode onemode \
         "PRST ShDisplay Fax Whiteboard Audio Video" %U


QuestionsMode.whatis:  action
QuestionsMode.pos:     -row 0 -column 4
QuestionsMode.title:   @ng/pques3.gif
QuestionsMode.balloon: Questions mode
QuestionsMode.cmd:     SetIMode quesmode "" %U  -ctrlsites *


DocCamera.whatis:   action
DocCamera.pos:      -row 0 -column 5
DocCamera.title:    @ng/pcamdoc.gif 
DocCamera.balloon:  Document camera presentation mode
DocCamera.cmd:      SetIMode doccamera \
          "ShDisplay Whiteboard PRST Audio Video" %U


VgaMode.whatis:   action
VgaMode.pos:      -row 0 -column 6
VgaMode.title:    @ng/pvdemo.gif
VgaMode.balloon:  VGA mode
VgaMode.cmd:      SetIMode vgamode "" %U -vgasite %U


AppSharingMode.whatis:   action
AppSharingMode.pos:      -row 0 -column 8
AppSharingMode.title:    @vnc2.gif
AppSharingMode.balloon:  Application sharing mode
AppSharingMode.cmd:      SetIMode appsharingmode \
              "Video Whiteboard PRST Audio ShDisplay" %U


SelectVncServer.whatis:    action
SelectVncServer.pos:       -row 0 -column 8
SelectVncServer.title:     @vnc2.gif
SelectVncServer.balloon:   Select local VNC server
SelectVncServer.cmd:       VNC VNCServersEditorC::ToggleMap


WhiteboardMode.whatis:  action
WhiteboardMode.pos:     -row 0 -column 11
WhiteboardMode.title:   @chalkbrd.gif
WhiteboardMode.balloon: Whiteboard mode
WhiteboardMode.cmd:     SetIMode whiteboardmode \
               "ShDisplay Video PRST Audio Whiteboard Notepad" %U

NotepadMode.whatis:  action
NotepadMode.pos:     -row 0 -column 12
NotepadMode.title:   @edit.gif
NotepadMode.balloon: Notepad mode
NotepadMode.cmd:     SetIMode notepadmode \
           "ShDisplay Video PRST Audio Notepad Whiteboard" %U


CreateAudio.whatis:  action
CreateAudio.pos:     -row 0 -column 14
CreateAudio.title:   @mezcla.gif
CreateAudio.balloon: Start/Show/Hide audio
CreateAudio.cmd:     CreateJoinHideShowComp %U Audio


Admon.whatis:    action
Admon.pos:       -row 0 -column 19
Admon.title:     @ng/tools.gif
Admon.balloon:   Open administration menu
Admon.cmd:       interface OpenMenuitemsMenu


QuitIsabel.whatis:    action
QuitIsabel.pos:       -row 0 -column 20
QuitIsabel.title:     @ng/door.gif
QuitIsabel.title2:    @ng/door_open.gif
QuitIsabel.balloon:   Exit ISABEL
QuitIsabel.cmd:       QuitSite





#--------------------------------------------------------------------
# INTERACTION MODE: appsharingmode
#--------------------------------------------------------------------

appsharingmode.whatis: imode

appsharingmode.Audio: -active 1 \
                  -codec "gsm-32KHz" \
                  -maxbw 300

appsharingmode.Video:  -active 1 \
                   -layout lappsharingmode \
                   -otherlayout {ol3lq ol4lq ol5lq ol10lq ol25lq ol36lq}

appsharingmode.VNC: -active 1 \
                    -layout lappsharingmode \
                    -shdisplaycodec  "MPEG-4 -qlty 50" \
                    -bw     400 \
                    -capturearea 800x600+200+1 0 1

# --------------------------------------------------------------------

Video.layout.lappsharingmode: \
 {video 192x144+000+000 250}

# --------------------------------------------------------------------

VNC.viewerlayout.lappsharingmode:    +200+1 0 1

# --------------------------------------------------------------------









# Component: Video
#--------------------------------------------------------------------

Video.whatis:        component    

Video.otherlayout:   
Video.othermap:      0

Video.conftables:    video videomq cameradoc videodemo vga

Video.ratefactor:    1

Video.vga.enabled:    0

#Video.FEC.K:         6

#--------------------------------------------------------------------

# Multiplos de 16:
#
#    64x48  128x96  192x144  256x192  320x240  384x288 448x336  512x384
#  575x432 640x480  704x528  768x576
#

Video.conftable.video: \
  {geometry=64x48   bw=0    {codec=MPEG-4 -qlty=0} grabsize=0       framerate=1} \
  {geometry=64x48   bw=15   {codec=MPEG-4 -qlty=0} grabsize=0       framerate=10} \
  {geometry=64x48   bw=50   {codec=MPEG-4 -qlty=0} grabsize=0       framerate=25} \
  {geometry=96x72   bw=0    {codec=MPEG-4 -qlty=0} grabsize=64x48   framerate=1} \
  {geometry=96x72   bw=15   {codec=MPEG-4 -qlty=0} grabsize=64x48   framerate=10} \
  {geometry=96x72   bw=50   {codec=MPEG-4 -qlty=0} grabsize=0       framerate=25} \
  {geometry=128x96  bw=0    {codec=MPEG-4 -qlty=0} grabsize=64x48   framerate=1} \
  {geometry=128x96  bw=15   {codec=MPEG-4 -qlty=0} grabsize=64x48   framerate=10} \
  {geometry=128x96  bw=50   {codec=MPEG-4 -qlty=0} grabsize=96x72   framerate=25} \
  {geometry=160x120 bw=0    {codec=MPEG-4 -qlty=0} grabsize=80x60   framerate=1} \
  {geometry=160x120 bw=15   {codec=MPEG-4 -qlty=0} grabsize=80x60   framerate=10} \
  {geometry=160x120 bw=50   {codec=MPEG-4 -qlty=0} grabsize=96x72   framerate=25} \
  {geometry=192x144 bw=0    {codec=MPEG-4 -qlty=0} grabsize=96x72   framerate=1} \
  {geometry=192x144 bw=15   {codec=MPEG-4 -qlty=0} grabsize=96x72   framerate=10} \
  {geometry=192x144 bw=50   {codec=MPEG-4 -qlty=0} grabsize=96x72   framerate=25} \
  {geometry=256x192 bw=0    {codec=MPEG-4 -qlty=0} grabsize=96x72   framerate=1} \
  {geometry=256x192 bw=15   {codec=MPEG-4 -qlty=0} grabsize=128x96  framerate=10} \
  {geometry=256x192 bw=50   {codec=MPEG-4 -qlty=0} grabsize=128x96  framerate=25} \
  {geometry=320x240 bw=0    {codec=MPEG-4 -qlty=0} grabsize=96x72   framerate=1} \
  {geometry=320x240 bw=15   {codec=MPEG-4 -qlty=0} grabsize=160x120 framerate=10} \
  {geometry=320x240 bw=50   {codec=MPEG-4 -qlty=0} grabsize=160x120 framerate=25} \
  {geometry=448x336 bw=0    {codec=MPEG-4 -qlty=0} grabsize=96x72   framerate=1} \
  {geometry=448x336 bw=15   {codec=MPEG-4 -qlty=0} grabsize=192x144 framerate=10} \
